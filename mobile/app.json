{
  "expo": {
    "name": "myapp",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "your-scheme",
    "userInterfaceStyle": "light",
     "jsEngine": "jsc",
     "sdkVersion": "53.0.0",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true
    },
    "android": {
      "package": "com.puritypath.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "newArchEnabled": true,
      "navigationBar": {
        "visible": "immersive"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "newArchEnabled": true,
      "tsconfigPaths": true
    },
    "_internal": {
      "isDebug": false,
      "fastRefresh": false,
      "newArchEnabled": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "764b3f23-4d64-44b3-bbcc-bbf555602735"
      }
    }
  }
}
